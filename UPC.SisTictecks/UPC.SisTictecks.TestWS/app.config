<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IServicios" />
                <binding name="BasicHttpBinding_IUsuarios" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:16005/Servicios.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IServicios" contract="ServiciosWS.IServicios"
                name="BasicHttpBinding_IServicios" />
            <endpoint address="http://localhost:16005/Usuarios.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUsuarios" contract="UsuarioWS.IUsuarios"
                name="BasicHttpBinding_IUsuarios" />
        </client>
    </system.serviceModel>
</configuration>